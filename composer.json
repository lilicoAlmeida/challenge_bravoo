{
  "name": "magazord/challenge-magazord",
  "description": "Desafio backend Magazord",
  "type": "project",
  "require": {
    "doctrine/orm": "^2.10",
    "vlucas/phpdotenv": "^5.5",
    "symfony/cache": "^6.0",
    "doctrine/annotations": "^1.13"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Src\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
